package aula2;

public class Main {

	public static void main(String[] args) {
		// 
		System.out.println("Ol√° mundo! ;)");
		
		//variaveis
		int valorA = 20;
		int valorB = 10;
		int valorC;
		valorC = valorA / valorB;
		System.out.println(valorC);
		//constantes 
		
		final float PI_NUMERO = 3.14f;
		double raio = 3;
		double circoferencia;
		circoferencia = raio / (PI_NUMERO * PI_NUMERO);
		
		System.out.println(circoferencia);
		
		
		
	}

}
